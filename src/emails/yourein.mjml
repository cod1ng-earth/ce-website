<mjml>
  <mj-head>
    <mj-title>Coding Earth Meetup</mj-title>
    <mj-preview>Coding Earth Meetup</mj-preview>
    <mj-attributes>
      <mj-all font-family="'Helvetica Neue', Helvetica, Arial, sans-serif"></mj-all>
      <mj-text font-weight="400" font-size="16px" color="#000000" line-height="24px" font-family="'Helvetica Neue', Helvetica, Arial, sans-serif"></mj-text>
    </mj-attributes>
    <mj-style inline="inline">
      .body-section { -webkit-box-shadow: 1px 4px 11px 0px rgba(0, 0, 0, 0.15); -moz-box-shadow: 1px 4px 11px 0px rgba(0, 0, 0, 0.15); box-shadow: 1px 4px 11px 0px rgba(0, 0, 0, 0.15); }
    </mj-style>
    <mj-style inline="inline">
      .text-link { color: #E06C00 }
    </mj-style>
    <mj-style inline="inline">
      .footer-link { color: #888888 }
    </mj-style>

  </mj-head>
  <mj-body background-color="#333333" width="600px">
    <mj-section full-width="full-width" background-color="#222222" padding-bottom="0">
      <mj-column width="100%">
        <mj-image src="data:image/png;base64," alt="" align="center" width="150px" />
        <mj-text color="#E06C00" align="center" font-size="13px" padding-top="0" font-weight="bold" text-transform="uppercase" letter-spacing="1px" line-height="20px">
          ###MEETUP_NAME###
        </mj-text>
      </mj-column>
    </mj-section>

    <mj-wrapper padding-top="0" padding-bottom="0" css-class="body-section">
      <mj-section background-color="#ffffff" padding-left="15px" padding-right="15px">
        <mj-column width="100%">
          <mj-text color="#212b35" font-weight="bold" font-size="20px">
            Confirming your attendance
          </mj-text>
          <mj-text color="#637381" font-size="16px">
            Hi ###USER_NAME###,
          </mj-text>
          <mj-text color="#637381" font-size="16px">
            This is for letting you know that you've registered for <b>###MEETUP_NAME###</b> successfully. We're happy to have you aboard this time :) 
          </mj-text>
          <mj-text color="#637381" font-size="16px">
            Here's your join link and a password to get into the session. Please make sure to <b>not</b> share it with anyone you don't trust to prevent Zoom bombers.
          </mj-text>
          <mj-text color="#637381" align="center" font-size="16px">
          	Link: <a href="###MEETUP_LINK###" class="text-link" >###MEETUP_LINK###</a>
          </mj-text>
          <mj-text color="#637381" align="center" font-size="16px">
          	Password: ###MEETUP_PASSWORD###
          </mj-text>
        </mj-column>
      </mj-section>
      
      <mj-section background-color="#ffffff" padding-left="15px" padding-right="15px" padding-top="0">
        <mj-column width="100%">
          <mj-divider border-color="#DFE3E8" border-width="1px" />
        </mj-column>
      </mj-section>
      <mj-section background-color="#ffffff" padding="0 15px 0 15px">
        <mj-column width="100%">

          <mj-text color="#637381" font-size="16px">
            We're looking forward meeting you.
          </mj-text>
          <mj-text color="#637381" font-size="16px">
            Stefan, Joao, Lars &amp; Thomas
          </mj-text>
        </mj-column>
      </mj-section>
      <mj-section background-color="#ffffff" padding-left="15px" padding-right="15px">
        <mj-column width="100%">
          <mj-text color="#212b35" font-size="12px" text-transform="uppercase" font-weight="bold" padding-bottom="0">
            ###MEETUP_NAME###
          </mj-text>
          <mj-text color="#637381" font-size="14px" padding-top="0">
            UTC (London): ###MEETUP_TIME_UTC### <br/>
            CET (Berlin): ###MEETUP_TIME_CET### <br/>
            WET (Lisbon): ###MEETUP_TIME_WET### <br/>
            ET  (New York): ###MEETUP_TIME_ET### <br/>
            PT  (Los Angeles): ###MEETUP_TIME_PT### <br/>
          </mj-text>
        </mj-column>
        <mj-column width="50%">
        </mj-column>
      </mj-section>
     
    </mj-wrapper>

    <mj-wrapper full-width="full-width">
      <mj-section>
        <mj-column width="100%" padding="0">
          <mj-social font-size="15px" icon-size="30px" mode="horizontal" padding="0" align="center">
            <mj-social-element name="github" href="http://github.com/cod1ng-earth/" background-color="#9E66D4">
            </mj-social-element>
            <mj-social-element name="twitter-noshare" href="https://twitter.com/coding_earth" background-color="#9E66D4">
            </mj-social-element>
          </mj-social>
          
          <mj-text color="#8040BF" font-size="12px" align="center" line-height="16px">
            You are receiving this email because you signed up for this event at https://coding.earth (If you think you got this email in error, let us know: <a href="https://coding.earth/imprint" style="color: #FF9900;">https://coding.earth/imprint</a>).
          </mj-text>
        </mj-column>
      </mj-section>
    </mj-wrapper>

  </mj-body>
</mjml>